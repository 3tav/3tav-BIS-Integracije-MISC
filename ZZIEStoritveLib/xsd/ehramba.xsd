				<xsd:element name="TIME_TO" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="STATUS">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="10"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>


	<xsd:element name="BATCH_DOC">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ID" type="PrimaryIDType"  minOccurs="0"/>
				<xsd:element name="DOC_EXT_ID">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="35"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="DOC_ID" type="PrimaryIDType"  minOccurs="0"/>
				<xsd:element name="TIME" type="xsd:dateTime"  minOccurs="0"/>
