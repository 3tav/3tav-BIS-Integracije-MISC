<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" >
	<xsd:include schemaLocation="LTAP-requestSub.xsd"></xsd:include>
	<xsd:element name="ArchiveData">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DataIdentification" minOccurs="0"	maxOccurs="1" />
				<xsd:element ref="Data" />
				<xsd:element ref="MetaData" />
                <xsd:element ref="ZZIInformation"  minOccurs="0"/>
				<xsd:element ref="DOCUMENT_INFO"  minOccurs="0"/>
				<xsd:element ref="DOCUMENTS"  minOccurs="0"/>
				<xsd:element ref="FOLDERS"  minOccurs="0"/>
				<xsd:element ref="DOCUMENT_VERSIONS"  minOccurs="0"/>
				<xsd:element ref="DOCUMENT_VERSION"  minOccurs="0"/>
				<xsd:element ref="DOCUMENT_FUNCTIONS"  minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
    <xsd:element name="ArchiveDataVer">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="InternalID" minOccurs="1" maxOccurs="1" />
                <xsd:element ref="VerDescription" minOccurs="1"
                    maxOccurs="1" />
                <xsd:element ref="ArchiveData" minOccurs="1" maxOccurs="1" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>  	
    <xsd:element name="ZZIInformation">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="InternalID" type="xsd:int"  minOccurs="0"/>
                <xsd:element name="Account" type="xsd:string"  minOccurs="0"/>
                <xsd:element name="UserID" type="xsd:int"  minOccurs="0"/>
                <xsd:element name="SubmitTime" type="xsd:dateTime"  minOccurs="0"/>
                <xsd:element name="FolderName" type="xsd:string"  minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="ArchiveDataDraft">
        <xsd:complexType>
            <xsd:sequence>
                    <xsd:element name="DraftID" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="UserCreated" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UserCreatedDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UserFor" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="UserForDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="GroupFor" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="GroupForDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="FolderName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="FolderDesc" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="DocSize" type="xsd:int" minOccurs="0"/>
                    
                    <xsd:element name="UserIdCreated" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="UserIdFor" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="GroupIdFor" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="FldId" type="xsd:int" minOccurs="0"/>
                    <xsd:element name="FsId" type="xsd:string" minOccurs="0"/>
                    
                    <xsd:element name="AccName" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="CreationTime" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="ExternalId" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Title" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Filename" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Mimetype" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="Type" type="xsd:string" minOccurs="0"/>
                    <xsd:element ref="ArchiveData" minOccurs="0"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
	<xsd:element name="DOCUMENTS_FUNCTIONS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DOCUMENT_FUNCTIONS" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:element name="DOCUMENT_FUNCTIONS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="INSERT" type="intYN"  minOccurs="0"/>
				<xsd:element name="EDIT" type="intYN"  minOccurs="0"/>
				<xsd:element name="DELETE" type="intYN"  minOccurs="0"/>
				<xsd:element name="VIEW" type="intYN"  minOccurs="0"/>
				<xsd:element name="VERIFY" type="intYN"  minOccurs="0"/>
				<xsd:element name="STATUS" type="intYN"  minOccurs="0"/>
				<xsd:element name="EXPORT" type="intYN"  minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>


	
	<xsd:simpleType name="intYN">
		<xsd:restriction base="xsd:int">
			<xsd:minExclusive value="0"></xsd:minExclusive>
			<xsd:maxExclusive value="1"></xsd:maxExclusive>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="DOCUMENT_INFO">
    	<xsd:complexType>
    		<xsd:sequence>
    			<xsd:element name="INSERT_DATE" minOccurs="1" maxOccurs="1" type="xsd:dateTime"/>
    			<xsd:element name="STATUS" minOccurs="1" maxOccurs="1" type="xsd:int"/>
    			<xsd:element name="SIZE" minOccurs="1" maxOccurs="1" type="xsd:int"/>
    		</xsd:sequence>
    	</xsd:complexType>
    </xsd:element>
    
	<xsd:element name="ACCOUNTS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ACCOUNT" minOccurs="1" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="ACCOUNT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID" minOccurs="0"/>
				<xsd:element ref="NAME" minOccurs="0"/>
				<xsd:element ref="DESCRIPTION" minOccurs="0" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELETE_DOCUMENTS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DELETE_DOCUMENT" minOccurs="1" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="DELETE_DOCUMENT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element name="USER">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="121"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="REASON">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="1024"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="TIME_CREATED" type="xsd:dateTime" minOccurs="0"/>
				<xsd:element name="TIME_RUN" type="xsd:dateTime" minOccurs="0"/>
				<xsd:element name="DOC_ID" type="xsd:int" minOccurs="0"/>
                <xsd:element name="EXTERNAL_ID" type="xsd:string"  minOccurs="0"/>
                <xsd:element name="TITLE" type="xsd:string"  minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DELETE_DOCUMENT_LISTS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="DELETE_DOCUMENT_LIST" minOccurs="1" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="DELETE_DOCUMENT_LIST">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ID" type="xsd:int"></xsd:element>
				<xsd:element name="USER">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="121"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="TITLE">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="1024"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="DESCRIPTION">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="4096"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="STATUS">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="10"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="TIME_CREATED" type="xsd:dateTime"></xsd:element>
				<xsd:element name="TIME_CONFIRMED" type="xsd:dateTime"></xsd:element>
				<xsd:element name="TIME_RUN" type="xsd:dateTime"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ACTION">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID">
					<xsd:annotation>
						<xsd:documentation>
							enak 0 - dodajanje nove akcije večji od 0 -
							spreminjanje obstoječe akcije
        		</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="NAME"  minOccurs="0"/>
				<xsd:element ref="DESCRIPTION" maxOccurs="1" minOccurs="0" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ACTIONS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ACTION" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="DESCRIPTION" type="DescriptionType"/>
	<xsd:element name="ID" type="PrimaryIDType"/>
	<xsd:simpleType name="DescriptionType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"></xsd:minLength>
			<xsd:maxLength value="255"></xsd:maxLength>
			<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PasswordType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="5"></xsd:minLength>
			<xsd:maxLength value="50"></xsd:maxLength>
			<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="IDType">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="1"></xsd:minInclusive>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:element name="NAME" type="NameType"/>

	<xsd:simpleType name="NameType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"></xsd:minLength>
			<xsd:maxLength value="32"></xsd:maxLength>
			<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="FRM_ID" type="IDType"></xsd:element>

	<xsd:simpleType name="PrimaryIDType">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"></xsd:minInclusive>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="ACT_ID" type="IDType"></xsd:element>

	<xsd:element name="GRU_ID" type="IDType"></xsd:element>

	<xsd:element name="ACTION_GROUP">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="ACT_ID"></xsd:element>
				<xsd:element ref="GRU_ID"></xsd:element>
				<xsd:element ref="CHECKED"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ACTIONS_GROUPS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ACTION_GROUP" minOccurs="1" maxOccurs="unbounded"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CHECKED" type="BooleanType"></xsd:element>

	<xsd:simpleType name="BooleanType">
		<xsd:restriction>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string"></xsd:restriction>
			</xsd:simpleType>
			<xsd:pattern
				value="(1?)|(0?)|((True)?)|((False)?)|((true)?)|((false)?)|((TRUE)?)|((FALSE)?)">
			</xsd:pattern>
			<xsd:minLength value="1"></xsd:minLength>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:element name="ACC_ID" type="IDType"></xsd:element>

	<xsd:element name="DOC_ID" type="IDType"></xsd:element>

	<xsd:element name="CLA_ID" type="IDType"></xsd:element>

	<xsd:element name="GROUPS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GROUP" minOccurs="1" maxOccurs="unbounded"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="GROUP">
		<xsd:complexType>
			<xsd:sequence>
			    <xsd:element ref="ID"></xsd:element>
			    <xsd:element ref="NAME"></xsd:element>
			    <xsd:element ref="DESCRIPTION"></xsd:element>
			    <xsd:element ref="DATE_FROM" minOccurs="0"></xsd:element>
			    <xsd:element ref="DATE_TO" minOccurs="0"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="DATE_FROM" type="xsd:dateTime"></xsd:element>

	<xsd:element name="DATE_TO" type="xsd:dateTime"></xsd:element>

	<xsd:element name="GROUPS_USERS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="GROUP_USER" minOccurs="1" maxOccurs="unbounded"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="GROUP_USER">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="USR_ID"></xsd:element>
				<xsd:element ref="GRU_ID"></xsd:element>
				<xsd:element ref="CHECKED"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="USR_ID" type="IDType"></xsd:element>

	<xsd:element name="CLASSIFICATIONS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CLASSIFICATION" minOccurs="1" maxOccurs="unbounded"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CLASSIFICATION">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="NAME"></xsd:element>
				<xsd:element ref="DESCRIPTION"></xsd:element>
				<xsd:element name="DOCUMENTS">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:length value="1"></xsd:length>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="ARCHIVE_PERIOD">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="3"></xsd:maxLength>
							<xsd:minLength value="1"></xsd:minLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="ARCHIVE_DOC" type="BooleanType"></xsd:element>
				<xsd:element name="EKEEPER" type="BooleanType"></xsd:element>
				<xsd:element name="EKEEPER_NAME">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="50"></xsd:maxLength>
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:pattern value="(group)|(individual)"></xsd:pattern>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="USERS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="USER" minOccurs="1" maxOccurs="unbounded" ></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
    
	<xsd:element name="USER">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="FRM_ID" minOccurs="1"></xsd:element>
				<xsd:element ref="USERNAME"></xsd:element>
				<xsd:element name="USR_PASSWORD" type="PasswordType"></xsd:element>
				<xsd:element ref="DATE_FROM" minOccurs="0"></xsd:element>
				<xsd:element ref="DATE_TO" minOccurs="0"></xsd:element>
				<xsd:element ref="MAIL" minOccurs="0"></xsd:element>
				<xsd:element ref="FIRST_NAME" minOccurs="0"></xsd:element>
				<xsd:element ref="LAST_NAME" minOccurs="0"></xsd:element>
				<xsd:element ref="FUNCTION" minOccurs="0"></xsd:element>
				<xsd:element ref="PHONE1" minOccurs="0"></xsd:element>
				<xsd:element ref="PHONE2" minOccurs="0"></xsd:element>
				<xsd:element ref="LANGUAGE"></xsd:element>
				<xsd:element ref="ACC_ID" minOccurs="1"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="USERNAME">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="50"></xsd:maxLength>
				<xsd:pattern value="[A-Za-z0-9\._]+"></xsd:pattern>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="MAIL">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="255"></xsd:maxLength>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="FIRST_NAME">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="60"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="LAST_NAME">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="60"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="PHONE1">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="40"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="PHONE2">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="40"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="LANGUAGE">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:length value="2"></xsd:length>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="FUNCTION">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="40"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="FIRMS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="FIRM" minOccurs="1" maxOccurs="unbounded" ></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="FIRM">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="NAME"></xsd:element>
				<xsd:element ref="TIN"></xsd:element>
				<xsd:element ref="ADDRESS" minOccurs="0"></xsd:element>
				<xsd:element ref="ZIP" minOccurs="0"></xsd:element>
				<xsd:element ref="CITY" minOccurs="0"></xsd:element>
				<xsd:element ref="COUNTRY" minOccurs="0"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ADDRESS">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="40"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="TIN">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:pattern value="\d{8}"></xsd:pattern>
				<xsd:length value="8"></xsd:length>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="ZIP">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="9"></xsd:maxLength>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="CITY">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="35"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="COUNTRY">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:maxLength value="35"></xsd:maxLength>
				<xsd:whiteSpace value="preserve"></xsd:whiteSpace>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<xsd:element name="CLASSIFICATIONS_FUNCTIONS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CLASSIFICATION_FUNCTION" minOccurs="1" maxOccurs="unbounded" ></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CLASSIFICATION_FUNCTION">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CLA_ID"></xsd:element>
				<xsd:element ref="INSERT"></xsd:element>
				<xsd:element ref="EDIT"></xsd:element>
				<xsd:element ref="DELETE"></xsd:element>
				<xsd:element ref="VIEW"></xsd:element>
				<xsd:element ref="VERIFY"></xsd:element>
				<xsd:element ref="STATUS"></xsd:element>
				<xsd:element ref="EXPORT"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CLASSIFICATIONS_GROUPS">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="CLASSIFICATION_GROUP" minOccurs="1" maxOccurs="unbounded"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CLASSIFICATION_GROUP">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ID"></xsd:element>
				<xsd:element ref="CLA_ID"></xsd:element>
				<xsd:element ref="GRU_ID"></xsd:element>
				<xsd:element ref="INSERT"></xsd:element>
				<xsd:element ref="EDIT"></xsd:element>
				<xsd:element ref="DELETE"></xsd:element>
				<xsd:element ref="VIEW"></xsd:element>
				<xsd:element ref="VERIFY"></xsd:element>
				<xsd:element ref="STATUS"></xsd:element>
				<xsd:element ref="EXPORT"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

