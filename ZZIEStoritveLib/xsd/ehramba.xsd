    <xsd:element name="DRAFT_DOCUMENTS">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element ref="DRAFT_DOCUMENT" minOccurs="0" maxOccurs="unbounded" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
    
	<xsd:element name="DRAFT_DOCUMENT">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ID" type="PrimaryIDType"  minOccurs="0"/>
				<xsd:element name="XML" type="xsd:string"  minOccurs="0"/>
				<xsd:element name="FS_ID">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="255"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CREATION_TIME" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="USR_ID_CREATED" type="xsd:int"  minOccurs="0"/>
                <xsd:element name="USER_CREATED" type="xsd:string"  minOccurs="0"/>
				<xsd:element name="USER_CREATED_DESC" type="xsd:string"  minOccurs="0"/>
                <xsd:element name="USR_ID_FOR" type="xsd:int"  minOccurs="0"/>
                <xsd:element name="USER_FOR" type="xsd:string"  minOccurs="0"/>
                <xsd:element name="USER_FOR_DESC" type="xsd:string"  minOccurs="0"/>
				<xsd:element name="GRU_ID_FOR" type="xsd:int"  minOccurs="0"/>
                <xsd:element name="GROUP_FOR" type="xsd:string"  minOccurs="0"/>
                <xsd:element name="GROUP_FOR_DESC" type="xsd:string"  minOccurs="0"/>
