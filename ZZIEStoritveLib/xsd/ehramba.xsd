						<xsd:maxLength value="255"></xsd:maxLength>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="EX_ROWS" type="xsd:int"></xsd:element>
			<xsd:element name="EX_COLS">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"></xsd:length>
						<xsd:enumeration value="0"></xsd:enumeration>
						<xsd:enumeration value="1"></xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="CODE_LIST_DISPLAY" type="xsd:int"></xsd:element>
			<xsd:element name="CODE_LIST_NAME">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="255"></xsd:length>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LABEL_EN">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="255"></xsd:length>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LABEL_SL">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="255"></xsd:length>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LABEL_SR">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="255"></xsd:length>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LABEL_DE">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="255"></xsd:length>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="LABEL_MK">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
