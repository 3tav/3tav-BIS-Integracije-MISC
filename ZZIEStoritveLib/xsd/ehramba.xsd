	<xsd:element name="PURGE">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ID" type="PrimaryIDType"  minOccurs="0"/>
				<xsd:element name="USR_ID_CREATED" type="xsd:int"  minOccurs="0"/>
				<xsd:element name="TITLE">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="1024"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="DESCRIPTION">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="4096"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="STATUS">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"></xsd:minLength>
							<xsd:maxLength value="10"></xsd:maxLength>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CREATED" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="CONFIRMED" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="SCHEDULED" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="COMPLETED" type="xsd:dateTime"  minOccurs="0"/>
				<xsd:element name="COUNT" type="xsd:int"  minOccurs="0"/>
				<xsd:element ref="PURGE_DOC" minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>



	<xsd:element name="BATCH">
